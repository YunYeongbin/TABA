<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.taba.repository.UserRepository">
    <select id="selectUser" parameterType="com.example.taba.model.User" resultType="com.example.taba.model.User">
        SELECT * FROM user1 WHERE user_id = #{user_id}
    </select>
    <insert id="insertUser" parameterType="com.example.taba.model.User">
        INSERT INTO user1(user_id, username, email, avatarUrl) VALUES(#{user_id}, #{username}, #{email}, #{avatarUrl}
    </insert>
</mapper>